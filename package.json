{
  "name": "image-resize-electron",
  "productName": "Image Resizer",
  "version": "1.0.0",
  "description": "an image resizing app",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "resize-img": "^2.0.0",
    "toastify-js": "^1.12.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@electron-forge/publisher-github": "^6.0.0-beta.67",
    "electron": "^21.1.0"
  },
  "author": "DevMirza",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_publish_example"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Zaid-maker",
              "name": "image-resizer",
              "draft": true
            }
          }
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zaid-maker/image-resizer.git"
  },
  "bugs": {
    "url": "https://github.com/Zaid-maker/image-resizer/issues"
  },
  "homepage": "https://github.com/Zaid-maker/image-resizer#readme"
}
